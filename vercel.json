{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    
    {
      "src": "/api/getAllCountries",
      "dest": "/api/getAllCountries.jsx"
    },
    {
      "src": "/api/getRegion/(.*)",
      "dest": "/api/getRegion.jsx"
    },
    {
      "src": "/api/getSearchCountry/(.*)",
      "dest": "/api/getSearchCountry.jsx"
    },
    {
      "src": "/api/getCountry/(.*)",
      "dest": "/api/getCountry.jsx"
    },
  
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
