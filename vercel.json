{
    "version": 2,
    "builds": [
      { "src": "vite.config.js", "use": "@vercel/static-build" }
    ],
    "routes": [
      {
        "src": "/api/getAllCountries",
        "dest": "/api/getAllCountries.js"
      },
      {
        "src": "/api/getRegion/(.*)",
        "dest": "/api/getRegion.js"
      },
      {
        "src": "/api/getSearchCountry/(.*)",
        "dest": "/api/getSearchCountry.js"
      },
      {
        "src": "/api/getCountry/(.*)",
        "dest": "/api/getCountry.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  